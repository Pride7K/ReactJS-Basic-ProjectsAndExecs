 #02\"></SectionTitle>\n            <div className=\"center\">\n                <div>\n                    <span className=\"text\">\n                        {text}\n                    </span>\n                    <div>\n                        <input type=\"text\" value={text} onChange={(e)=> setText(e.target.value) } className=\"input\"/>\n                    </div>\n                </div>\n                <div>\n                    <span className=\"text\">{number}</span>\n                    <div>\n                        <button className=\"btn\" onClick={() => setNumber(number + 1)}>+1</button>\n                        <button className=\"btn\" onClick={() => setNumber(number - 1)}>-1</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default UseContext\n","C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\views\\examples\\UseCustom.jsx",["103"],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\components\\layout\\PageTitle.jsx",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\components\\layout\\SectionTitle.jsx",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\views\\examples\\useCallBackButtons.jsx",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\data\\DataContext\\DataContext.js",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\data\\Store\\Store.jsx",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\Store\\Config\\index.js",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\Store\\Actions\\Number\\Number.js",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\Store\\Reducers\\Index\\Index.js",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\hooks\\useCounter\\useCounter.js",[],"C:\\Users\\gsant\\Desktop\\React\\react-hooks\\exec\\src\\hooks\\useFetch\\useFetch.js",[],{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","replacedBy":"107"},{"ruleId":"108","severity":1,"message"